timezone: UTC

+td-result-into-azureblobstorage:
  td>: queries/sample.sql
  database: sample_datasets
  result_connection: your_connection_name
  result_settings:
    container: container_name
    path_prefix: /path/file_${moment(session_time).format("YYYYMMDD")}
    file_ext: csv.gz
    format: csv
    compression: 'gz'
    delimiter: ","
    null_string: ""
    newline: CRLF
